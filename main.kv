                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0.8}
                    text: "Monod"
                    on_release:
                        app.root.current = "Monod_MP1"
                        root.manager.transition.direction = "left"

                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0.4}
                    text: "Andrews"
                    on_release:
                        app.root.current = "Andrews_MP1"
                        root.manager.transition.direction = "left"

                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0.0}
                    text: "Levenspiel"
                    on_release:
                        app.root.current = "Levenspinel_MP1"
                        root.manager.transition.direction = "left"

        MDFloatLayout:
            MDRoundFlatButton:
                pos_hint: {'center_x': 0.5, 'top':0.3}
