<DrawerClickableItem@MDNavigationDrawerItem>
    focus_color: "#e7e4c0"
    text_color: "#4a4939"
    icon_color: "#4a4939"
    ripple_color: "#c5bdd2"
    selected_color: "#0c6c4d"


<DrawerLabelItem@MDNavigationDrawerItem>
    text_color: "#4a4939"
    icon_color: "#4a4939"
    focus_behavior: False
    selected_color: "#4a4939"
    _no_ripple_effect: True

WindowManager:
    Home:

    Modelagem:

    Escolha_M1:
    Monod_MP1:
    Andrews_MP1:
    Levenspinel_MP1:

    Escolha_M2:
    Monod_MP2:
    Andrews_MP2:
    Levenspinel_MP2:

    Escolha_M3:
    Monod_MP3:
    Andrews_MP3:
    Levenspinel_MP3:

    Simulacao:
    Escolha_S1:
    Monod_SP1:
    Andrews_SP1:
    Levenspinel_SP1:

    Escolha_S2:
    Monod_SP2:
    Andrews_SP2:
    Levenspinel_SP2:

    Escolha_S3:
    Monod_SP3:
    Andrews_SP3:
    Levenspinel_SP3:


<Home>:
    name: "home"
    MDBoxLayout:
        orientation: 'vertical'
        MDTopAppBar:
            title: "Home"
            left_action_items:[['menu', lambda x: nav_drawer.set_state("open")]]

        MDBoxLayout:
            orientation: 'vertical'
            size: root.width, root.height
            MDBoxLayout:
                pos_hint: {'center_x': 0.5, 'top':0.8}
                MDLabel:
                    text: "\n\n\n Bem vindo ao SiMoBio!\n\n\n Este programa realiza simulações e modelagens de processos fermentativos de um modo bem intuitivo! Para isso, basta acessar o menu no canto superior esquerdo da tela e escolher sua função de interesse.\n\n Caso tenha alguma dúvida sobre o embasamento teórico ou de como utilizar o programa corretamente, acesse o site do projeto onde estão dispostos os conceitos teóricos e matemáticos por trás das simulações modelagens, além de um tutorial sobre como realizá-las. Para acessar o site, basta clicar no botão da documentação logo abaixo."
                    halign: 'center'
                    padding_x: 20
        MDBoxLayout:
            orientation: 'horizontal'
            size: root.width, root.height
            MDBoxLayout:
                MDFloatLayout:
                    MDRoundFlatButton:
                        pos_hint: {'center_x': 0.5, 'top':0.8}
                        size_hint_y: 0.15
                        size_hint_x: 0.5
                        height: 30
                        text: 'Documentação'
                        on_release: root.documentacao()

    MDNavigationDrawer:
        id: nav_drawer
        radius: (0, 16, 16, 0)
        MDNavigationDrawerMenu:
            MDNavigationDrawerHeader:
                title: "SiMoBio"
                title_color: "#4a4939"
                text: "Menu"
                source: "logo_simobio5.png"
                spacing: "4dp"
                padding: "12dp", 0, 0, "56dp"

            MDNavigationDrawerLabel:
                text: "Onde ir?"

            DrawerClickableItem:
                #icon: "gmail"
                text_right_color: "#4a4939"
                text: "Home"
                #on_press:
                    #app.root.current = "home"
                    #root.manager.transition.direction = "right"

            MDNavigationDrawerDivider:

            DrawerClickableItem:
                #icon: "send"
                text: "Simulação"
                on_press:
                    nav_drawer.set_state("close")
                    app.root.current = "simulacao"
                    root.manager.transition.direction = "left"

            MDNavigationDrawerDivider:

            DrawerClickableItem:
                text: "Modelagem"
                on_press:
                    nav_drawer.set_state("close")
                    app.root.current = "modelagem"
                    root.manager.transition.direction = "left"

<Modelagem>:
    name: "modelagem"
    MDBoxLayout:
        orientation: 'vertical'
        MDTopAppBar:
            left_action_items:[['menu', lambda x: nav_drawer.set_state("open")]]
            title: "Modelagem"

        MDBoxLayout:
            orientation: 'vertical'
            MDFloatLayout:
                MDLabel:
                    pos_hint: {'top': 1}
                    text: "Qual o tipo de produto do processo?"
                    halign: 'center'
            MDFloatLayout:
                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':1.25}
                    text: "Associado ao crescimento"
                    on_release:
                        app.root.current = "Escolha_M1"
                        root.manager.transition.direction = "left"

                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':1.05}
                    text: "Não associado ao crescimento"
                    on_release:
                        app.root.current = "Escolha_M2"
                        root.manager.transition.direction = "left"

                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0.85}
                    text: "Parcialmente associado ao crescimento"
                    on_release:
                        app.root.current = "Escolha_M3"
                        root.manager.transition.direction = "left"

    MDNavigationDrawer:
        id: nav_drawer
        radius: (0, 16, 16, 0)
        MDNavigationDrawerMenu:
            MDNavigationDrawerHeader:
                title: "SiMoBio"
                title_color: "#4a4939"
