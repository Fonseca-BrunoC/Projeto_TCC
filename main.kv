<LoadDialog>:
    MDBoxLayout:
        orientation: 'vertical'
        pos: root.pos
        size: root.size
        FileChooserIconView:
            id: loadfile
            on_selection: root.load(loadfile.path, loadfile.selection), root.cancel()


        MDBoxLayout:
            size_hint_y: 0.08
            MDFloatLayout:
                MDRectangleFlatButton:
                    pos_hint:{'center_x':0.5, 'center_y':0.5}
                    text: "Cancel"
                    on_release: root.cancel()


