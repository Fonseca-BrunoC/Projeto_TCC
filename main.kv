WindowManager:
    Home:
    Modelagem:
    MP1:
    MP2:
    MP3:
    Simulacao:
    SP1:
    SP2:
    SP3:


<Home>:
    name: "home"
    MDBoxLayout:
        orientation: 'horizontal'
        size: root.width, root.height
        MDBoxLayout:
            MDFloatLayout:
                MDRoundFlatButton:
                    pos_hint: {'center_x': 0.5, 'top':0.6}
                    size_hint_y: 0.15
                    size_hint_x: 0.5
                    height: 30
                    text: 'Simulação'
                    on_release:
                        app.root.current = "simulacao"
                        root.manager.transition.direction = "right"

        MDBoxLayout:
            MDFloatLayout:
                MDRoundFlatButton:
                    pos_hint: {'center_x': 0.5, 'top':0.6}
                    size_hint_y: 0.15
                    size_hint_x: 0.5
                    height: 30
                    text: 'Modelagem'
                    on_release:
                        app.root.current = "modelagem"
                        root.manager.transition.direction = "left"


<Modelagem>:
    name: "modelagem"
    MDBoxLayout:
        orientation: 'vertical'
        MDBoxLayout:
            orientation: 'vertical'
            MDFloatLayout:
                MDLabel:
                    pos_hint: {'center_x': 0.85, 'top': 0.8}
                    text: "Qual o tipo de produto do processo?"
            MDFloatLayout:
                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0.8}
                    text: "Associado ao crescimento"
                    on_release:
                        app.root.current = "MP1"
                        root.manager.transition.direction = "left"
                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0.4}
                    text: "Não associado ao crescimento"
                    on_release:
                        app.root.current = "MP2"
                        root.manager.transition.direction = "left"
                MDRectangleFlatButton:
                    pos_hint:{'center_x': 0.5, 'top':0}
                    text: "Parcialmente associado ao crescimento"
                    on_release:
                        app.root.current = "MP3"
                        root.manager.transition.direction = "left"


        MDFloatLayout:
            MDRoundFlatButton:
                pos_hint: {'center_x': 0.5, 'top':0.3}
                size_hint_y: 0.15
                size_hint_x: 0.2
                height: 30
                text: 'Voltar'
                on_release:
                    app.root.current = "home"
                    root.manager.transition.direction = "right"

