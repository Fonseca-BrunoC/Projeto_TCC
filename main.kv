<SP1>:
    name: 'SP1'
    MDBoxLayout:
        orientation: 'vertical'
        MDFloatLayout:
            MDTextField:
                id: t1
                hint_text: "Tempo"
                helper_text: "Tempo em horas de duração do processo"
                helper_text_mode: "persistent"
                pos_hint: {"center_x":0.55, "center_y":0.5}
        MDFloatLayout:
            MDTextField:
                id: cx1
                hint_text: "Cxi"
                helper_text: "Concentração inicial de células em g/L"
                helper_text_mode: "persistent"
                pos_hint: {"center_x":0.55, "center_y":0.5}
        MDFloatLayout:
            MDTextField:
                id: cs1
                hint_text: "Csi"
                helper_text: "Concentração inicial de substrato em g/L"
                helper_text_mode: "persistent"
                pos_hint: {"center_x":0.55, "center_y":0.5}
        MDFloatLayout:
            MDTextField:
                id: mimax1
                hint_text: "mimax"
                helper_text: "Velocidade específica máxima de crescimento em 1/h"
                helper_text_mode: "persistent"
                pos_hint: {"center_x":0.55, "center_y":0.5}
